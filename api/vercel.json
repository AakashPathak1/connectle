{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ],
  "env": {
    "ADMIN_SECRET": {
      "type": "secret"
    },
    "FLASK_ENV": "production",
    "SUPABASE_URL": {
      "type": "secret"
    },
    "SUPABASE_KEY": {
      "type": "secret"
    }
  }
}
